{
  "name": "comments",
  "version": "0.1.0",
  "private": true,
  "browserslist": [
    "extends @nice-digital/browserslist-config"
  ],
  "volta": {
    "node": "14.19.0"
  },
  "engines": {
    "node": ">=8.11.1",
    "npm": ">=5.7.1"
  },
  "scripts": {
    "start": "rimraf ./build && npm run modernizr && npm-run-all -p sass:watch start-js",
    "start-js": "react-app-rewired start",
    "build": "npm run modernizr && npm run sass:build && react-app-rewired build",
    "test": "react-app-rewired test",
    "eslint": "eslint src/ --fix",
    "esw": "node node_modules/eslint-watch/bin/esw 'src/' --watch",
    "test:ci": "cross-env CI=true react-app-rewired test --env=jsdom --testResultsProcessor=jest-teamcity-reporter",
    "test:ci:coverage": "npm run test:ci -- --coverage ",
    "eject": "react-scripts eject",
    "flow": "flow",
    "modernizr": "modernizr -c modernizr-config.json -d ./public/vendor/modernizr-custom.js",
    "sass:build": "npm-run-all -s sass:compile sass:post",
    "sass:compile": "sass -I ./node_modules src/main.scss public/styles/main.css",
    "sass:post": "postcss public/styles/*.css --use autoprefixer -d public/styles",
    "sass:watch": "npm run sass:build && sass -I ./node_modules src/main.scss public/styles/main.css --watch"
  },
