{
  "name": "comments",
  "version": "0.1.0",
  "private": true,
  "browserslist": [
    "extends @nice-digital/browserslist-config"
  ],
  "engines": {
    "node": ">=8.11.1",
    "npm": ">=5.7.1"
  },
  "scripts": {
    "start": "rimraf ./build && npm run modernizr && npm-run-all -p sass:watch start-js",
    "start-js": "react-scripts start",
    "build": "npm run modernizr && npm run sass:build && npm run bless && react-scripts build",
    "test": "react-scripts test --env=node",
    "bless": "blessc ./public/styles/main.css --output ./public/styles/main-ie9.css",
    "eslint": "eslint src/ --fix",
    "esw": "node node_modules/eslint-watch/bin/esw 'src/' --watch",
    "test:ci": "cross-env CI=true react-scripts test --env=jsdom --testResultsProcessor=jest-teamcity-reporter",
    "test:ci:coverage": "npm run test:ci -- --coverage ",
    "eject": "react-scripts eject",
    "flow": "flow",
    "modernizr": "modernizr -c modernizr-config.json -d ./public/vendor/modernizr-custom.js",
    "sass:build": "npm-run-all -s sass:compile sass:post bless",
    "sass:compile": "node-sass-chokidar --include-path ./node_modules/@nice-digital/design-system/src/stylesheets --include-path ./node_modules/@nice-digital/icons/dist --source-map true src/main.scss -o public/styles --importer=node_modules/node-sass-tilde-importer",
    "sass:post": "postcss public/styles/*.css --use autoprefixer -d public/styles",
    "sass:watch": "npm run sass:build && chokidar \"./src/**/*.scss\" -c \"npm run sass:build\""
  },
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-flow": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "@babel/register": "^7.10.4",
    "@nice-digital/nds-alert": "^1.0.22-alpha.0",
    "@nice-digital/nds-button": "^1.0.22-alpha.0",
    "@nice-digital/nds-input": "^1.0.3-alpha.0",
    "aspnet-prerendering": "^3.0.1",
    "axios": "^0.21.1",
    "babel-preset-react-app": "^9.1.2",
    "bless": "^3.0.3",
    "classlist-polyfill": "^1.2.0",
    "ie9-oninput-polyfill": "^1.1.1",
    "ignore-styles": "^5.0.1",
    "js-cookie": "^2.2.0",
    "lodash": "^4.17.10",
    "lodash-deeper": "^1.1.0",
    "moment": "^2.22.1",
    "object-hash": "^1.3.0",
    "pdfmake": "^0.1.66",
    "query-string": "4.3.4",
    "react": "^16.13.1",
    "react-aria-live": "^2.0.2",
    "react-copy-to-clipboard": "^5.0.2",
    "react-debounce-input": "^3.2.0",
    "react-dom": "^16.13.1",
    "react-helmet": "^5.2.0",
    "react-html-parser": "^2.0.2",
    "react-markdown": "^4.1.0",
    "react-moment": "^0.7.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-router-hash-link": "^1.2.0",
    "stringify-object": "^3.2.2"
  },
  "devDependencies": {
    "@nice-digital/browserslist-config": "^1.0.0",
    "@nice-digital/design-system": "^0.5.1",
    "@nice-digital/eslint-config": "^1.0.2",
    "@nice-digital/icons": "^1.5.1",
    "axios-mock-adapter": "^1.15.0",
    "chokidar-cli": "^1.2.1",
    "cross-env": "^5.2.0",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "enzyme-to-json": "^3.3.4",
    "eslint-watch": "^3.1.5",
    "flow": "^0.2.3",
    "flow-bin": "^0.75.0",
    "jest-teamcity-reporter": "^0.9.0",
    "jsdom": "^11.10.0",
    "modernizr": "3.5.0",
    "node-sass-chokidar": "1.4.0",
    "node-sass-tilde-importer": "^1.0.2",
    "npm-run-all": "^4.1.3",
    "postcss-cli": "^5.0.0",
    "raf": "^3.4.0",
    "react-perf-devtool": "^3.1.8",
    "react-scripts": "3.4.1",
    "rimraf": "^2.6.2",
    "why-did-you-update": "^0.1.1"
  },
  "proxy": "http://niceorg",
  "description": "This project was bootstrapped with [Create React App](https://github.com/facebookincubator/create-react-app).",
  "main": "index.js",
  "keywords": [],
